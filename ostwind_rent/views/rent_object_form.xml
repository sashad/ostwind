<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ostwind_rent_object_form_view" model="ir.ui.view">
        <field name="name">Rent property</field>
        <field name="model">ostwind.rent.object</field>
        <field name="arch" type="xml">
            <form>
                <header/>
                <sheet string="Properties">
                    <widet name="web_ribbon" text="Archived" bg_color="text-bg-danger" invisible="active"/>
                    <field name="active" invisible="1"/>
                    <div class="oe_title">
                        <h1>
                            <field name="name" required="1" placeholder="Name ..."/>
                        </h1>
                    </div>
                    <group name="studio_group_d2e547">
                        <group name="studio_group_d2e547_left">
                            <field name="addr_street"/>
                            <field name="addr_house"/>
                            <field name="addr_zip"/>
                            <field name="addr_city"/>
                            <field name="addr_country_id"/>
                        </group>
                        <group name="studio_group_d2e547_right"/>
                    </group>
                    <notebook name="studio_notebook_837_1ibu4295p">
                        <page string="General" name="studio_page_837">
                            <group name="studio_group_837">
                                <group name="studio_group_837_left" string="Property">
                                    <field name="property_type"/>
                                    <field name="date_build" placeholder="1990" string="Year of construction"/>
                                    <field name="plot_number"/>
                                    <field name="cadastral_municipality"/>
                                    <field name="entry_number"/>
                                </group>
                                <group name="studio_group_837_right" string="Co-ownership">
                                    <field name="coownership"/>
                                </group>
                            </group>
                        </page>
                        <page string="Units" name="studio_page_600_1ibu454aa">
                            <group name="studio_group_600">
                                <group name="studio_group_600_left">
                                    <!--
                                    <field name="x_studio_beschreibung" string="Description" placeholder="Units assigned to the property"/>
                                    <field name="x_studio_one2many_property_units" string="Units"/>
                                    -->
                                </group>
                                <group name="studio_group_600_right"/>
                            </group>
                        </page>
                        <page string="Heating/Value" name="studio_page_5rn_1ibu5h9ff">
                            <group name="studio_group_5rn">
                                <group name="studio_group_5rn_left">
                                    <field name="heating_requirement"/>
                                    <field name="heating_type"/>
                                    <field name="energy_source"/>
                                    <field name="energy_certificate" string="Energy certificate"/>
                                    <field widget="file" filename="energy_certificate_file" name="energy_certificate_file"/>
                                    <field name="billing_company"/>
                                    <field name="heating_type_comment" string="Heating type comments"/>
                                </group>
                                <group name="studio_group_5rn_right">
                                <field name="utility_value" string="utility value" help="Value according to land register" options="{&quot;human_readable&quot;:false,&quot;type&quot;:&quot;m2&quot;}"/>
                                    <field name="net_floor_area" string="Net Floor Area (NFA)"/>
                                    <field name="heated_living_aria" string="Heated living area" help="Area or space that can be heated"/>
                                    <field name="variable_cost_share" string="Variable cost share"/>
                                    <field name="fixed_cost_share" string="Fixed cost share"/>
                                </group>
                            </group>
                        </page>
                    </notebook>                    
                </sheet>
            </form>
        </field>
    </record>
</odoo>
