<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ostwind_rent_model_action" model="ir.actions.act_window">
        <field name="name">Rent objects</field>
        <field name="res_model">ostwind.rent.object</field>
        <field name="view_mode">kanban,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Define a new rent object.
            </p>
        </field>
    </record>

    <record id="action_ostwind_rent_object_kanban" model="ir.actions.act_window.view">
        <field name="sequence" eval="0"/>
        <field name="view_mode">kanban</field>
        <field name="view_id" ref="ostwind_rent_object_kanban_view"/>
        <field name="act_window_id" ref="ostwind_rent_model_action"/>
    </record>    
    
    <record id="action_ostwind_rent_object_form" model="ir.actions.act_window.view">
        <field name="sequence" eval="2"/>
        <field name="view_mode">form</field>
        <field name="view_id" ref="ostwind_rent_object_form_view"/>
        <field name="act_window_id" ref="ostwind_rent_model_action"/>
    </record>

    <record id="menu_ostwind_rent" model="ir.ui.menu">
        <field name="name">Rent objects</field>
        <field name="action" ref="ostwind_rent_model_action"/>
    </record>
</odoo>
